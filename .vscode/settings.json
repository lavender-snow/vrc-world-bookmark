{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.words": [
    "Akar",
    "asar",
    "AUTOINCREMENT",
    "Elems",
    "fadein",
    "Hugeicons",
    "Iconoir",
    "Mynaui",
    "relocator",
    "Tabler",
    "Uncategorized",
    "vrchat",
    "wrld"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "prettier.useTabs": false
}
