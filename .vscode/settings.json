{
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "cSpell.words": [
        "AUTOINCREMENT",
        "Elems",
        "fadein",
        "Iconoir",
        "Uncategorized",
        "vrchat",
        "wrld",
        "asar"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    }
}
